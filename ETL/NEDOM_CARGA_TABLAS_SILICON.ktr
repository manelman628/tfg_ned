    <sql>SELECT 
tract.ambulatorio_fk_id id, to_char(tract.fechaingreso,&apos;YYYY-MM-DD&apos;) &quot;data_ingres&quot;,
to_char(tract.fechaalta,&apos;YYYY-MM-DD&apos;) &quot;data_alta&quot;, tract.suspendido suspes, tract.observaciones obs_tractament, 
to_char(tract.confirmadohasta,&apos;YYYY-MM-DD HH24:MI&apos;) &quot;confirmat_fins&quot;, tract.pendienteconfirmar pendent_confirmar, to_char(tract.validohasta,&apos;YYYY-MM-DD HH24:MI&apos;) &quot;valid_fins&quot;, tract.etcid etc_id,
to_char(tract.fechaproceso,&apos;YYYY-MM-DD HH24:MI:SS&apos;) &quot;data_proces&quot;, 
pac.NHC, pac.cip, pac.nif, pac.nombre nom_pacient , pac.apellido cognoms_pacient,  to_char(pac.fechanacimiento,&apos;YYYY-MM-DD&apos;) data_naixement,  to_char(pac.fechaexitus,&apos;YYYY-MM-DD&apos;) data_exitus,
pac.direccion adressa, pac.poblacion poblacio, pac.codigopostal codi_postal, pac.provincia, pac.nacionalidad nacionalitat, pac.telefono1 telefon, pac.sexo sexe,
LISTAGG(CONCAT(CONCAT(ant.tipo,&apos; - &apos;), ant.descripcion), &apos;; &apos;) WITHIN GROUP (ORDER BY ant.descripcion) antecedents,
usu.descripcion usuari, usu.login usuari_login, usu.grupo_descripcion usuari_grup, LISTAGG(DISTINCT serv.descripcion, &apos;, &apos;) WITHIN GROUP (ORDER BY serv.descripcion) &quot;usuari_servei&quot;,
usu.grupo_rol usuari_rol, usu.medico_numerocolegiado usuari_num_colegiat, 
usu.email usuari_email, usu.medico_codigo usuari_codi_metge, usu.activo usuari_actiu, 
serv.codigo servei_codi, serv.descripcion servei
FROM SILDOX_VC.edu_ambulatorio tract 
INNER JOIN SILDOX_VC.edu_paciente pac ON pac.paciente_id = tract.pacientes_fk
LEFT JOIN SILDOX_VC.edu_usuario usu ON usu.usuario_id = tract.medico_fk
LEFT JOIN SILDOX_VC.edu_servicio serv ON serv.servicio_id = tract.servicio_fk AND serv.descripcion &lt;&gt; &apos;No definido&apos;
LEFT JOIN SILDOX_VC.edu_pacientesantecedentes ant ON ant.pacientes_fk = pac.paciente_id
WHERE (tract.programamedicacion_fk = 34 AND tract.servicio_fk IN (5,27,28,29)) OR (tract.ambulatorio_fk_id IN (
633477,
591302))
GROUP BY tract.ambulatorio_fk_id, tract.fechaingreso,
tract.fechaalta, tract.suspendido, tract.observaciones, tract.confirmadohasta, tract.pendienteconfirmar,tract.validohasta,tract.etcid,tract.fechaproceso,
pac.NHC, pac.cip, pac.nif, pac.nombre, pac.apellido,  pac.fechanacimiento,pac.fechaexitus,
pac.direccion, pac.poblacion, pac.codigopostal, pac.provincia, pac.nacionalidad, pac.telefono1, pac.sexo,
usu.descripcion, usu.login, usu.grupo_descripcion, usu.grupo_rol, usu.medico_numerocolegiado, 
usu.email, usu.medico_codigo, usu.activo, serv.codigo, serv.descripcion
ORDER BY tract.ambulatorio_fk_id asc</sql>
