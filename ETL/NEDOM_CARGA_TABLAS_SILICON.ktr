    <connection>mirror_SILICON_10_50_202_71</connection>
    <sql>SELECT
prescrip.tratamiento_fk id_tractament, prescrip.prescripmedicamento_id id_prescripcio, prescrip.prescripcioninicial_fk id_prescripcio_inicial,
indi.descripc indicacio, indi.codigo codi_indicacio,
prescrip.fechainicio data_inici, prescrip.fechafin data_fi, prescrip.validohasta valida_fins, prescrip.fechahastatomas data_fins_toma, 
prescrip.consejosadministracion consells_admin, 
medi.descripcion medi_desc, pac.cip cip_prescripcio,
prescrip.via_fk, via.nombre via_codi, via.descripcion via_descripcio, 
prescrip.frecuencia_fk, freq.codigo freq_codi, freq.descripcion freq_descripcio, freq.tipofrecuenciadescripcion freq_tipus,
prescrip.DOSISPRESCRITA dosi, usrmed.descripc usuari_prescripcio, 
LISTAGG(CONCAT(esp.CODIGONACIONAL, &apos;, &apos;)) WITHIN GROUP (ORDER BY esp.CODIGONACIONAL) codigonacional
FROM SILDOX_VC.EDU_PRESCRIPMEDICAMENTO prescrip
INNER JOIN SILDOX_VC.edu_ambulatorio tract ON tract.ambulatorio_fk_id = prescrip.tratamiento_fk
INNER JOIN SILDOX_VC.edu_medicamentoasistencial medi ON medi.medicamentoasistencial_id=prescrip.medicamento_fk
INNER JOIN SILDOX_VC.edu_paciente pac ON pac.paciente_id = tract.pacientes_fk
LEFT JOIN SILDOX_VC.indicacionesprescripcion indi on indi.idindicacionesprescrip = prescrip.motivoprescripcion_fk
LEFT JOIN SILDOX_VC.edu_via via on via.via_id = prescrip.via_fk
LEFT JOIN SILDOX_VC.edu_frecuenciaprescripcion freq on freq.frecuencia_id = prescrip.frecuencia_fk
LEFT JOIN SILDOX_VC.usuarios usrmed ON usrmed.idusuari = prescrip.usuariosmedico_fk
LEFT JOIN SILDOX_VC.edu_especialidadasistencial esp on medi.medicamentoasistencial_id=esp.medicamentoasistencial_fk
WHERE (tract.programamedicacion_fk = 34 AND tract.servicio_fk IN (5,27,28,29)) OR (tract.ambulatorio_fk_id IN (
633477,
591302))
GROUP BY prescrip.tratamiento_fk, prescrip.prescripmedicamento_id, prescrip.prescripcioninicial_fk,
indi.descripc, indi.codigo,
prescrip.fechainicio, prescrip.fechafin, prescrip.validohasta, prescrip.fechahastatomas, 
prescrip.consejosadministracion, medi.descripcion, pac.cip,
prescrip.via_fk, via.nombre, via.descripcion,prescrip.frecuencia_fk, freq.codigo, freq.descripcion, freq.tipofrecuenciadescripcion,
prescrip.DOSISPRESCRITA, usrmed.descripc
ORDER BY  prescrip.prescripmedicamento_id ASC, prescrip.tratamiento_fk ASC</sql>
